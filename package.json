{
  "name": "raconteur",
  "version": "0.0.1",
  "description": "A toolset for converting markdown and json to blog posts.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run create && npm run convert",
    "clean": "npm run clean-build && npm run clean-public",
    "clean-build": "rm -rf ./lib",
    "clean-public": "rm -rf ./public",
    "create": "npm run create-lib",
    "create-lib": "(mkdir ./lib || true)",
    "convert": "npm run convert-bare && npm run convert-cli && npm run convert-test && npm run convert-utils",
    "convert-bare": "./node_modules/.bin/coffee --bare --compile --output ./lib/ src/coffee/bare",
    "convert-cli": "./node_modules/.bin/coffee --compile --output ./lib/ src/coffee/cli",
    "convert-test": "./node_modules/.bin/coffee --compile --output ./test/ src/coffee/test",
    "convert-utils": "./node_modules/.bin/coffee --compile --output ./lib/ src/coffee/utils",
    "test": "npm run build && ./node_modules/.bin/mocha --reporter spec",
    "prepublish": "npm run build",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brekk/raconteur.git"
  },
  "keywords": [
    "blog",
    "markdown",
    "writing",
    "platform",
    "template",
    "view",
    "dust",
    "jade"
  ],
  "author": "Brekk Bockrath",
  "license": "ISC",
  "dependencies": {
    "ampersand-model": "~4.0.2",
    "body-parser": "~1.9.0",
    "chalk": "^0.5.1",
    "cheerio": "~0.18.0",
    "coffee-script": "~1.8.0",
    "cp-r": "^0.1.2",
    "debug": "^2.1.1",
    "del": "^1.1.1",
    "dustjs-linkedin": "~2.4.2",
    "express": "~4.9.7",
    "jade": "~1.7.0",
    "json-front-matter": "~0.1.6",
    "marked": "^0.3.2",
    "minimist": "^1.1.0",
    "mkdirp": "^0.5.0",
    "parkplace": "0.0.10",
    "promised-io": "~0.3.4",
    "rantjs": "^0.9.1",
    "request": "~2.53.0",
    "slug": "~0.8.0",
    "wrap-stream": "^2.0.0"
  },
  "devDependencies": {
    "browserify": "^5.11.2",
    "chalk": "~0.5.1",
    "del": "^0.1.3",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^1.0.0",
    "gulp-clean": "^0.3.1",
    "gulp-coffee": "^2.2.0",
    "gulp-concat": "^2.4.1",
    "gulp-download": "0.0.1",
    "gulp-dust": "^1.0.1",
    "gulp-flatten": "0.0.3",
    "gulp-footer": "^1.0.5",
    "gulp-header": "^1.0.5",
    "gulp-jade": "^0.8.0",
    "gulp-minify-css": "^0.3.8",
    "gulp-mocha": "^1.1.0",
    "gulp-newer": "^0.3.0",
    "gulp-notify": "^1.6.0",
    "gulp-plumber": "^0.6.5",
    "gulp-rename": "^1.2.0",
    "gulp-rsync": "0.0.4",
    "gulp-stylus": "^1.3.2",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^1.0.4",
    "gulp-yuidoc": "^0.1.2",
    "lodash": "^2.4.1",
    "mocha": "^2.2.1",
    "optimist": "^0.6.1",
    "rantjs": "^0.9.1",
    "should": "^4.4.2",
    "source-map": "~0.1.39",
    "streamqueue": "^0.1.1",
    "through2": "^0.6.1",
    "uglify-js": "^2.4.16",
    "vinyl": "^0.4.5",
    "vinyl-fs": "~0.3.7",
    "vinyl-source-stream": "^1.0.0",
    "yuidocjs": "~0.3.50"
  }
}
